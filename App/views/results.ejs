          <% }); %>
        </tr>
    </div>
    <br>
        <div>
          <button type="button" id="getEvents" >View All Events</button>
          <button type="button" id="newQuery" >Perform another query</button>
        </div>
    <br>
    
</body>

<script src="js/web3.min.js"></script>
<script src="js/app.js"></script>
</html>